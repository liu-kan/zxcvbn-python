msgid "%s second"
msgstr "%s second"

#: zxcvbn/time_estimates.py:61
#, python-format
msgid "%s minute"
msgstr "%s minute"

#: zxcvbn/time_estimates.py:64
#, python-format
msgid "%s hour"
msgstr "%s hour"

#: zxcvbn/time_estimates.py:67
#, python-format
msgid "%s day"
msgstr "%s day"

#: zxcvbn/time_estimates.py:70
#, python-format
msgid "%s month"
msgstr "%s month"

#: zxcvbn/time_estimates.py:73
#, python-format
msgid "%s year"
msgstr "%s year"

#: zxcvbn/time_estimates.py:75
msgid "centuries"
msgstr "centuries"
